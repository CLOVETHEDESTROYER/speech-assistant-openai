# Add this to your .env file

# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key

# Twilio Credentials
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# Twilio Voice Intelligence API
TWILIO_VOICE_INTELLIGENCE_SID=your_voice_intelligence_sid
USE_TWILIO_VOICE_INTELLIGENCE=false  # Set to true to use Twilio Voice Intelligence, false to use OpenAI Whisper
ENABLE_PII_REDACTION=true  # Set to true to enable PII redaction in Twilio Voice Intelligence

# Database Configuration
DATABASE_URL=sqlite:///./sql_app.db

# JWT Authentication
SECRET_KEY=your_secret_key
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Public URL (for Twilio webhook)
PUBLIC_URL=your_public_url  # e.g., https://your-domain.com or ngrok URL for local development

# Voice Settings
VOICE_ID=alloy  # Available options: alloy, echo, fable, onyx, nova, shimmer
VOICE_MODEL=tts-1  # tts-1 or tts-1-hd

# Logging Level
LOG_LEVEL=INFO 